			"smart-contracts/StekcitBMEmissions.sol": {
				"content": "// SPDX-License-Identifier: MIT\n\npragma solidity ^0.8.19;\n\ncontract StekcitBMEmissions {\n     event EvStekcitUser(\n        uint256 id,\n        address walletAddress,\n        string username,\n        string emailAddress,\n        bool isCreatingUser,\n        bool isBlank\n    );\n\n    event EvStekcitEvent(\n        uint256 id,\n        address creatingUserWalletAddress,\n        string description,\n        string link,\n        uint256 amountInEthers,\n        uint256 createdAt,\n        uint256 updatedAt,\n        uint256 dateAndTime,\n        bool isBlank,\n        bool isPublished,\n        bool isVerified,\n        uint256 verificationAmountInEthers,\n        bool isEnded,\n        bool isPaidOut\n    );\n\n    event EvStekcitTicket(\n        uint256 id,\n        uint256 eventId,\n        address attendingUserWalletAddress,\n        uint256 amountPaidInEthers,\n        bool isBlank\n    );\n\n    event EvStekcitPayout(\n        uint256 id,\n        uint256 eventId,\n        address creatingUserWalletAddress,\n        uint256 amountPaidOutInEthers,\n        uint256 dateTimeAndMade,\n        bool isBlank\n    );\n}\n"
			}
		},
